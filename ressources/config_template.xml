<config>
    <version>2</version>
    <ArduinoVersion>#ArduinoVersion#</ArduinoVersion>
    <ArduinoQty>#ArduinoQty#</ArduinoQty>
    <!-- Serial -->
    <A1_serial_port>#A1_serial_port#</A1_serial_port>
    <A2_serial_port>#A2_serial_port#</A2_serial_port>
    <A3_serial_port>#A3_serial_port#</A3_serial_port>
    <A4_serial_port>#A4_serial_port#</A4_serial_port>
    <A5_serial_port>#A5_serial_port#</A5_serial_port>
    <A6_serial_port>#A6_serial_port#</A6_serial_port>
    <A7_serial_port>#A7_serial_port#</A7_serial_port>
    <A8_serial_port>#A8_serial_port#</A8_serial_port>
    <!-- Trigger -->
    <trigger_url>#trigger_url#</trigger_url>
    <trigger_nice>#trigger_nice#</trigger_nice>
    <apikey>#apikey#</apikey>
    <!-- Daemon -->
    <daemon_pidfile>#pid_path#</daemon_pidfile>
    <!-- Logging -->
    <loglevel>error</loglevel>
    <logfile>#log_path#.error</logfile>
    <!-- Socket server -->
    <sockethost>#sockethost#</sockethost>
    <socketport>#socketport#</socketport>
</config>